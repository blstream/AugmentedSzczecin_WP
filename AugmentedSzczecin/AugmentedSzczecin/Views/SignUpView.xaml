        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0.18*"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="0.18*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="0.25*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <ScrollViewer 
            Grid.Column="1" 
            Grid.Row="1"
            IsEnabled="{Binding AreControlsEnabled}">
            <StackPanel>
                <TextBlock 
                    x:Uid="SignUpMainText" 
                    FontSize="25"
                    HorizontalAlignment="Center"
                    Margin="0,0,0,25"/>
                <Border 
                    Height="50"
                    CornerRadius="10" 
                    BorderThickness="2" 
                    BorderBrush="{StaticResource ASBlue}">
                    <TextBox 
                    x:Name="Email" 
                    x:Uid="SignEmail"
                    caliburn:Message.Attach="[Event KeyDown] = [Action EmailTextBoxChangeFocusToPasswordTextBox($executionContext)]" 
                    Margin="5,5,5,0" 
                    Style="{StaticResource TextBoxStyleEmail}" Background="{x:Null}" BorderThickness="0" Foreground="{StaticResource ASBlue}" 
                    />
                </Border>
                <TextBlock Margin="0,0,0,5"/>
                <Border 
                    Height="50"
                    CornerRadius="10" 
                    BorderThickness="2" 
                    BorderBrush="{StaticResource ASBlue}">
                    <PasswordBox 
                        x:FieldModifier="Public" 
                        x:Name="Password" 
                        x:Uid="SignPassword"
                        Margin="5,5,5,0" 
                        BorderThickness="0"
                        Background="{x:Null}"
                        Foreground="{StaticResource ASBlue}" Style="{StaticResource PasswordBoxStylePassword}" />
                </Border>
                <Border
                    Margin="10,40,10,20"
                    CornerRadius="10" 
                    Height="40"
                    BorderThickness="2" 
                    BorderBrush="{StaticResource ASRed}" Background="{StaticResource ASRed}">
                    <Button 
                        x:Name="Register" 
                        x:Uid="SignUpButtonSignUp" 
                        Height="40"
                        HorizontalAlignment="Stretch" BorderBrush="{x:Null}" Style="{StaticResource ButtonStyleRegister}" Background="{StaticResource ASRed}"  />
                </Border>
            </StackPanel>
        </ScrollViewer>
        <StackPanel 
                Grid.Column="1"
                Grid.Row="2">
            <ProgressRing 
                x:Name="SignUpProgressRing" 
                IsActive="{Binding IsProgressRingActive}"
                Visibility="{Binding IsProgressRingVisible, Converter={StaticResource BoolToVisConv}}" 
                Height="10" 
                Width="200" 
                HorizontalAlignment="Center" Style="{StaticResource ProgressRingStyleSign}"/>
            <TextBlock 
                x:Uid="SignUpProgressRing" 
                FontSize="15" 
                Visibility="{Binding IsProgressRingVisible, Converter={StaticResource BoolToVisConv}}" 
                HorizontalAlignment="Center" 
                Foreground="{StaticResource ASRed}"
                Margin="0,10,0,0" />
        </StackPanel>
    </Grid>

</Page>
